{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "GOOGLE_CSE_KEY": "@google_cse_key",
    "GOOGLE_CSE_CX": "@google_cse_cx"
  }
}
